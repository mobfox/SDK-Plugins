<extension xmlns="http://ns.adobe.com/air/extension/16.0">     <id>com.mobfox.air.plugin</id>     <versionNumber>0.0.1</versionNumber>     <platforms>         <platform name="Android-ARM">            <applicationDeployment>                <nativeLibrary>androidmobfoxpluginlib.jar</nativeLibrary>                <initializer>com.mobfox.air.plugin.MobFoxPluginExtension</initializer>            </applicationDeployment>         </platform>		<platform name="iPhone-ARM">             <applicationDeployment>                 <nativeLibrary>libiOSMobFoxPluginLib.a</nativeLibrary>                 <initializer>MobFoxPluginExtensionInitializer</initializer>            </applicationDeployment>        </platform>        <platform name="default">            <applicationDeployment/>        </platform>    </platforms> </extension>