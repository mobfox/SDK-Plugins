package com.mobfox.air.plugin.events{	import flash.events.Event;		public class MobFoxPluginEvent extends Event	{		//----------------------------------------		//		// Constants		//		//----------------------------------------				public static const BANNER_ERROR:String    = "bannerError";		public static const BANNER_READY:String    = "bannerReady";		public static const BANNER_CLOSED:String   = "bannerClosed";		public static const BANNER_CLICKED:String  = "bannerClicked";		public static const BANNER_FINISHED:String = "bannerFinished";				public static const INTERSTITIAL_READY:String    = "interstitialReady";		public static const INTERSTITIAL_ERROR:String    = "interstitialError";		public static const INTERSTITIAL_CLOSED:String   = "interstitialClosed";		public static const INTERSTITIAL_FINISHED:String = "interstitialFinished";		public static const INTERSTITIAL_CLICKED:String  = "interstitialClicked";		public static const INTERSTITIAL_SHOWN:String    = "interstitialShown";				public static const NATIVE_READY:String    = "nativeReady";		public static const NATIVE_ERROR:String    = "nativeError";				//----------------------------------------		//		// Params		//		//----------------------------------------				public var eventData:String = "";				//----------------------------------------		//		// Constructor		//		//----------------------------------------		public function MobFoxPluginEvent( type:String, data:String, bubbles:Boolean=false, cancelable:Boolean=false ) {			eventData = data;			super(type, bubbles, cancelable);		}	}}